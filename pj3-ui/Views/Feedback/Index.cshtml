@{
	ViewData["Title"] = "Feedback";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<!doctype html>
<html lang="zxx">

<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- Template CSS -->
	<link rel="stylesheet" href="~/assets/css/style-starter.css">
	<!-- Template CSS -->
	<link href="//fonts.googleapis.com/css?family=Poppins:300,400,400i,500,600,700&display=swap" rel="stylesheet">
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
</head>

<body>

	<nav id="breadcrumbs" class="breadcrumbs">
		<div class="container page-wrapper">
			<a href="index.html">Home</a> » <span class="breadcrumb_last" aria-current="page">QuoteUs</span>
		</div>
	</nav>
	<!-- //contact-form -->
	<!-- /contact-form -->
	<section class="w3l-contact-main">
		<div class="contact-infhny py-5">
			<div class="container py-lg-5">
				<div class="title-content text-left mb-lg-5 mt-4">
					<h6 class="sub-title">Quote Us</h6>
					<h3 class="hny-title">Drop your feedback<br>or ask us something.</h3>
				</div>
				<div class="row align-form-map">
					<div class="col-lg-6 form-inner-cont">
						<div class="signin-form">

							<div class="form-input">
								<label for="fullName">Full Name*</label>
								<input type="text" name="fullName" id="FullName" placeholder="" />
								<div class="error text-danger"></div>
							</div>
							<div class="form-input">
								<label for="companyName">Company Name*</label>
								<input type="text" name="companyName" id="CompanyName" placeholder=""/>
								<div class="error text-danger"></div>
							</div>

							<div class="form-input">
								<label for="email">Email*</label>
								<input type="email" name="email" id="Email" placeholder=""/>
								<div class="error text-danger"></div>
							</div>

							<div class="form-input">
								<label for="phoneNumber">Phone number*</label>
								<input type="text" name="phoneNumber" id="PhoneNumber" placeholder=""/>
								<div class="error text-danger"></div>
							</div>
							<div class="form-input">
								<label for="subject">Subject*</label>
								<input type="text" name="subject" id="Subject" placeholder=""/>
								<div class="error text-danger"></div>
							</div>
							<div class="form-input">
								<label for="comment">Comment*</label>
								<textarea placeholder="" name="comment" id="Comment"></textarea>
								<div class="error text-danger"></div>
							</div>

							<button id="btnSubmit" class="btn btn-contact">Submit</button>
						</div>


					</div>
					<div class="map col-lg-6 pl-lg-4">
						<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3001161.424489281!2d-78.01909140705047!3d42.72866436845163!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4ccc4bf0f123a5a9%3A0xddcfc6c1de189567!2sNew%20York%2C%20USA!5e0!3m2!1sen!2sin!4v1570786994395!5m2!1sen!2sin"
								frameborder="0" allowfullscreen=""></iframe>
					</div>
				</div>
			</div>
	</section>
	<!-- //contact-form -->
	<!-- footer-66 -->
	<!--//footer-66 -->
</body>

</html>
<script src="~/js/Feedback/Index.js"></script>